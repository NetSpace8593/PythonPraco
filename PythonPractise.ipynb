    "#How to sum of the first N Positive Integer in Python\n",
    "num = int(input(\"How many numbers?\"))\n",
    "total_sum = 0\n",
    "\n",
    "for n in range (num):\n",
    "    numbers = float(input(\"Enter any number\"))\n",
    "    total_sum += numbers\n",
    "    \n",
    "avg = total_sum / num\n",
    "print(\"Average is:\", avg)"
